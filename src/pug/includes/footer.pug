include contacts

footer.footer
	.footer__top
		.container
			.footer__wrap.footer__wrap--top
				a.btn.btn--logo(href="/")
					picture.img.img--logo
						source.img__item(srcset='img/logo.webp' type="image/webp")
						source.img__item(srcset='img/logo.jp2' type="image/jp2")
						img.img__item(src="img/logo.png" alt="logo")
							
				+contacts('footer')
	.footer__bottom
		.container
			nav.nav.nav--footer
				
				- const footerMenu = ["Anfahrt", "Datenschutzerkl√§rung", "Impressum"];
				
				ul.nav__menu.nav__menu--footer
					each footerMenuItem in footerMenu
						li.nav__item.nav__item--footer
							a.nav__link.nav__link--footer.text.text--small-cont(href="#")=footerMenuItem
